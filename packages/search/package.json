{
  "dependencies": {
    "@kos-kit/mem-models": "2.0.5",
    "@kos-kit/models": "2.0.5",
    "@kos-kit/rdf-resource": "2.0.5",
    "@rdfjs/types": "^1.1.0",
    "@types/lunr": "^2.3.7",
    "axios": "^1.7.2",
    "lunr": "^2.3.9"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "files": [
    "*.d.ts",
    "*.d.ts.map",
    "*.js",
    "*.js.map",
    "LICENSE",
    "package.json"
  ],
  "license": "GPL-3.0-only",
  "name": "@kos-kit/search",
  "scripts": {
    "build": "tsc -b && cp LICENSE package.json dist/",
    "clean": "rimraf dist",
    "link": "cd dist && npm link",
    "publish:dist": "cd dist && npm publish --access public",
    "rebuild": "rimraf dist && tsc -b && cp LICENSE package.json dist/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "unlink": "npm unlink -g @kos-kit/search",
    "watch": "mkdir -p dist && cp LICENSE package.json dist/ && tsc -w --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kos-kit/lib"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "2.0.5"
}
