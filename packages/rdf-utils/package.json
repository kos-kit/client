{
  "dependencies": {
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@rdfjs/term-set": "^1.1.0",
    "@rdfjs/types": "^1.1.0",
    "@types/rdfjs__term-set": "^1.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "files": [
    "*.d.ts",
    "*.d.ts.map",
    "*.js",
    "*.js.map",
    "LICENSE",
    "package.json"
  ],
  "license": "GPL-3.0-only",
  "name": "@kos-kit/rdf-utils",
  "scripts": {
    "build": "tsc -b && cp LICENSE package.json dist/",
    "clean": "rimraf dist",
    "link": "cd dist && npm link",
    "publish:dist": "cd dist && npm publish --access public",
    "rebuild": "rimraf dist && tsc -b && cp LICENSE package.json dist/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "unlink": "npm unlink -g @kos-kit/rdf-utils",
    "watch": "mkdir -p dist && cp LICENSE package.json dist/ && tsc -w --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kos-kit/lib"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "2.0.6"
}
