    "base-x": "^3.0.9",
    "envalid": "^8.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "files": [
    "**/*.ts"
  ],
  "license": "GPL-3.0-only",
  "name": "@kos-kit/next-utils",
  "scripts": {
    "build": "tsc -b && cp LICENSE package.json dist/",
