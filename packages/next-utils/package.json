    "rebuild": "rimraf dist && tsc -b && cp LICENSE package.json dist/",
    "unlink": "npm unlink -g @kos-kit/next-utils",
    "watch": "mkdir -p dist && cp LICENSE package.json dist/ && tsc -w --preserveWatchOutput"
